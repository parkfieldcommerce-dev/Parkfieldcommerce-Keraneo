.timeline-section .timeline {
        position: relative;
        max-width: 100%;
        margin: 0 auto;
        margin-top: 20px;
        padding: 1em 0;
        list-style-type: none;
    }
    .timeline-section .timeline:before {
        position: absolute;
        left: 50%;
        top: 0;
        content: ' ';
        display: block;
        width: 1px;
        height: 100%;
        margin-left: -3px;
        background: #707070;
        /* background: -moz-linear-gradient(top, rgb(0,157,221,0) 0%, rgb(0,157,221) 8%, rgb(0,157,221) 92%, rgb(0,157,221,0) 100%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(30,87,153,1)), color-stop(100%,rgba(125,185,232,1)));
        background: -webkit-linear-gradient(top, rgb(0,157,221,0) 0%, rgb(0,157,221) 8%, rgb(0,157,221) 92%, rgb(0,157,221,0) 100%);
        background: -o-linear-gradient(top, rgb(0,157,221,0) 0%, rgb(0,157,221) 8%, rgb(0,157,221) 92%, rgb(0,157,221,0) 100%);
        background: -ms-linear-gradient(top, rgb(0,157,221,0) 0%, rgb(0,157,221) 8%, rgb(0,157,221) 92%, rgb(0,157,221,0) 100%);
        background: linear-gradient(to bottom, rgb(0,157,221,0) 0%, rgb(0,157,221) 8%, rgb(0,157,221) 92%, rgb(0,157,221,0) 100%); */
        
        z-index: 5;
    }
    .timeline-section .timeline li {
        padding: 1em 0;
    }
    .timeline-section .timeline li:after {
        content: "";
        display: block;
        height: 0;
        clear: both;
        visibility: hidden;
    }
    .timeline-section .direction-l {
        position: relative;
        width: 50%;
        float: left;
        text-align: right;
        right: 30px;
    }
    .timeline-section .direction-r {
        position: relative;
        width: 50%;
        float: right;
        left: 30px;
    }
    .timeline-section .flag-wrapper {
        position: relative;
        display: inline-block;
        
        text-align: center;
    }
    .timeline-section .flag {
        position: relative;
        display: inline;
        background: #000;
        padding: 6px 10px;
        border-radius: 5px;
        font-family: "Archivo Narrow";
        font-size: 48px;
        font-weight: bold;
        text-align: left;
        text-transform: uppercase;
    }
    .timeline-section .direction-l .flag {
        -webkit-box-shadow: -1px 2px 3px rgba(0,0,0,0.15), 0 0 1px rgba(0,0,0,0.15);
        -moz-box-shadow: -1px 2px 3px rgba(0,0,0,0.15), 0 0 1px rgba(0,0,0,0.15);
        box-shadow: -1px 2px 3px rgba(0,0,0,0.15), 0 0 1px rgba(0,0,0,0.15);
    }
    .timeline-section .direction-r .flag {
        -webkit-box-shadow: 1px 2px 3px rgba(0,0,0,0.15), 0 0 1px rgba(0,0,0,0.15);
        -moz-box-shadow: 1px 2px 3px rgba(0,0,0,0.15), 0 0 1px rgba(0,0,0,0.15);
        box-shadow: 1px 2px 3px rgba(0,0,0,0.15), 0 0 1px rgba(0,0,0,0.15);
    }
    .timeline-section .direction-l .flag:before,
    .timeline-section .direction-r .flag:before {
        position: absolute;
        top: 50%;
        right: -45px;
        content: ' ';
        display: block;
        width: 35px;
        height: 35px;
        margin-top: -10px;
        background: #F4F5F5;
        border-radius: 50%;
        border: 1px solid #707070;
        z-index: 10;
    }
    .timeline-section .direction-l .flag.xl:before {
        position: absolute;
        top: 50%;
        right: -47.5px;
        content: ' ';
        display: block;
        width: 25px;
        height: 25px;
        margin-top: -15px;
        background: #F4F5F5;
        border-radius: 20px;
        border: 5px solid #009DDD;
        z-index: 10;
    }
    .timeline-section .direction-r .flag:before {
        left: -48px;
    }
    .timeline-section .direction-l .flag:after {
        content: "";
        position: absolute;
        left: 100%;
        top: 50%;
        height: 0;
        width: 0;
        margin-top: -8px;
        border: solid transparent;
        border-left-color: rgb(248,248,248);
        border-width: 8px;
        pointer-events: none;
    }
    .timeline-section .direction-r .flag:after {
        content: "";
        position: absolute;
        right: 100%;
        top: 50%;
        height: 0;
        width: 0;
        margin-top: -8px;
        border: solid transparent;
        border-right-color: rgb(248,248,248);
        border-width: 8px;
        pointer-events: none;
    }
    .timeline-section .flag.wbg {
        background: transparent;
        border: none;
        box-shadow: none;
